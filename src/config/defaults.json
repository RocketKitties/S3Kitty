{
	"colors": [
		"red", "orange", "gold", "yellow", "green", "teal", "aqua", "cyan", "blue", "violet",
		"magenta", "raspberry", "brown", "grey"
	],

	"contact": {
		"description": "For questions and comments, please fill out the contact form below: ",
		"subjects": [
			"Question",
			"Comment",
			"Feature Request",
			"Setting Up My Own Instance",
			"Bug Report",
			"Other"
		],
		"label": "Submit Question"
	},

	"desktop": {
		"show_app_launcher": true,
		"preferences": {
			"desktop_apps": {
				"phone": ["file_browser"],
				"tablet": ["file_browser"],
				"desktop": ["file_browser"]
			},
			"admin_apps": {
				"phone": ["app_launcher"],
				"tablet": ["app_launcher"],
				"desktop": ["account_manager", "file_browser"]
			},
			"pinned_apps": []
		}
	},

	"dialogs": {
		"sizes": {
			"tiny": [600, 400],
			"small": [720, 480],
			"medium": [900, 600],
			"large": [1200, 800]
		},
		"scale": [0.75, 0.75],
		"margin": [100, 100]
	},

	"help": {
		"show_app": true,
		"show_pdf": false,
		"show_topic": true,
		"show_contact": true
	},

	"downloads": {
		"production": {
			"2025-02-07": "downloads/S3Kitty_2025-02-07.zip"
		},
		"development": {
			"GitHub": "https://github.com/RocketKitties/S3Kitty"
		}
	},

	"search": {
		"directory": "/Shared",
		"placeholder": "Search",
		"hidden": true
	},

	"settings": {
		"enable_notifications": true,
		"mute_sounds": false,
		"volume": 1
	},

	"sharing": {
		"emoji": true,
		"files": {
			"Files": {
				"icon": "fa fa-file",
				"directory": ""
			},
			"Audio": {
				"icon": "fa fa-volume-up",
				"directory": "Audio/"
			},
			"Music": {
				"icon": "fa fa-music",
				"directory": "Music/"
			},
			"Pictures": {
				"icon": "fa fa-image",
				"directory": "Pictures/"
			},
			"Videos": {
				"icon": "fa fa-video",
				"directory": "Videos/"
			},
			"Maps": {
				"icon": "fa fa-map",
				"directory": "Maps/"
			}
		},
		"uploads": true,
		"locations": true
	},

	"text": {

		"titles": [
			".title", ".jqplot-title",
			".app-bar",
			".context-menu",
			".heading",
			".popover-title",
			".dropzone",
			".app th", ".modal th", 
			".app .help.message"
		],

		"labels": [
			"label",
			".header label",
			".dropdown .form-label",
			".app .form-label", ".modal .form-label",
			".app .control-label", ".modal .control-label",
			".menu-bar .dropdown-toggle",
			".menu-bar .dropdown-menu li a",
			".nav-tabs",
			".btn",
			"input",
			"select"
		],

		"headings": [
			"h1", "h2", "h3"
		],

		"content": [
			"p", ".section li"
		]
	}
}